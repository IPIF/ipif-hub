    "version": "0.2.0",
    "configurations": [
 


        {
            "name": "Python: Ingest Management Command",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "ingest_from_file",
                "SAMPLE_DATA.json"
            ],
            "django": true,
            "justMyCode": true
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
            "justMyCode": true
        },
        {
            "name": "Python: Celery",
            "type": "python",
            "request": "launch",
            "module": "celery",
            "console": "integratedTerminal",
            "args": [
                "-A",
                "djangobaseproject",
                "worker",
                "-l",
                "info",
            ]
        }
        
    ]
}